{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "fg": "#ecf0c1",
    "bg": "#0f111b",
    "bgdark": "#000000",
    "purple": "#7a5ccc",
    "blue": "#00a3cc",
    "green": "#5ccc96",
    "yellow": "#f2ce00",
    "red": "#e33400",
    "lavender": "#b3a1e6",
    "comment": "#818bb3",
    "selection": "#1b1c36"
  },
  "theme": {
    "primary": {
      "dark": "purple",
      "light": "purple"
    },
    "secondary": {
      "dark": "blue",
      "light": "blue"
    },
    "accent": {
      "dark": "lavender",
      "light": "lavender"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "yellow",
      "light": "yellow"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "blue",
      "light": "blue"
    },
    "text": {
      "dark": "fg",
      "light": "fg"
    },
