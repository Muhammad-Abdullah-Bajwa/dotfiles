      "dark": "purple",
      "light": "purple"
    },
    "syntaxFunction": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxVariable": {
      "dark": "fg",
      "light": "fg"
    },
    "syntaxString": {
      "dark": "green",
      "light": "green"
    },
    "syntaxNumber": {
      "dark": "lavender",
      "light": "lavender"
    },
    "syntaxType": {
      "dark": "yellow",
      "light": "yellow"
    },
    "syntaxOperator": {
      "dark": "purple",
      "light": "purple"
    },
    "syntaxPunctuation": {
      "dark": "fg",
      "light": "fg"
